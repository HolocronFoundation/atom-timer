{
  "name": "tomatimer",
  "main": "./lib/tomatimer",
  "version": "1.2.2",
  "description": "A loose and straightforward pomodoro timer.",
  "keywords": [
    "pomodoro",
    "tomato timer",
    "tomato",
    "timer"
  ],
  "activationCommands": {
    "atom-workspace": "tomatimer:start"
  },
  "standard": {
    "globals": [
      "atom",
      "Audio",
      "describe",
      "beforeEach",
      "afterEach",
      "it",
      "expect",
      "window"
    ]
  },
  "repository": "https://github.com/Yu1989/atom-tomatimer",
  "license": "MIT",
  "engines": {
    "atom": ">=1.0.0 <2.0.0"
  },
  "dependencies": {
    "lodash": "^4.12.0"
  },
  "consumedServices": {
    "status-bar": {
      "versions": {
        "^1.0.0": "consumeStatusBar"
      }
    }
  }
}
